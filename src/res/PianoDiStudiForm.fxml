<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="1000" prefWidth="1000" xmlns="http://javafx.com/javafx/8"
          fx:controller="portale.Studente.boundaries.PianoDiStudiForm">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="311.0" minWidth="10.0" prefWidth="228.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="464.0"/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="189.0" minHeight="10.0" prefHeight="164.0" vgrow="SOMETIMES"/>
        <RowConstraints prefHeight="318.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="100.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <GridPane GridPane.columnSpan="2" GridPane.rowIndex="1">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="306.0" minWidth="10.0" prefWidth="190.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="458.0" minWidth="10.0" prefWidth="430.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="450.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="300.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <TableView fx:id="pianoDiStudiTV" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2">
                    <GridPane.margin>
                        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
                    </GridPane.margin>
                </TableView>
                <TableView fx:id="appelliTV" prefHeight="200.0" prefWidth="641.0" visible="false"
                           GridPane.columnSpan="2" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
                    </GridPane.margin>
                </TableView>
            </children>
        </GridPane>
        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true"
                   GridPane.halignment="CENTER">
            <image>
                <Image url="@logo_unipa.gif"/>
            </image>
            <GridPane.margin>
                <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
            </GridPane.margin>
        </ImageView>
        <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#clickLogout" text="Logout"
                GridPane.halignment="LEFT" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
            </GridPane.margin>
        </Button>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="UNIVERSITÁ DEGLI STUDI DI PALERMO" wrappingWidth="400.0"
              GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
            </GridPane.margin>
            <font>
                <Font size="40.0"/>
            </font>
        </Text>
    </children>
</GridPane>
